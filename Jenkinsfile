pipeline {
  agent any
  stages {
    stage('Preparation') {
      steps {
        echo 'It is a preparation step.'
      }
    }
    stage('Build') {
      steps {
        echo 'Project will be build here.'
      }
    }
    stage('Test') {
      steps {
        echo 'Test will be done here.'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deployment will be done here.'
      }
    }
  }
}